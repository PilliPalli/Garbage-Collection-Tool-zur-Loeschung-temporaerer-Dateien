<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Garbage_Collector.ViewModel"
                    xmlns:view="clr-namespace:Garbage_Collector.View">

    <DataTemplate DataType="{x:Type vm:RegisterVM}">
        <view:Register />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}">
        <view:Login />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CleanupVM}">
        <view:Cleanup />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InformationVM}">
        <view:Information />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdministrationVM}">
        <view:Administration/>
    </DataTemplate>
                  

</ResourceDictionary>
    
    
    
    
    